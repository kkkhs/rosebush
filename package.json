{
  "name": "rosebush-react",
  "version": "0.1.5",
  "description": "React components library",
  "private": false,
  "author": "kkkhs",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React"
  ],
  "homepage": "https://rosebush.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/kkkhs/rosebush"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "react-transition-group": "^4.4.5",
    "rollup": "^2.79.1"
  },
  "peerDependencies": {
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "clean": "rimraf ./dist",
    "build": " npm run clean && npm run build-ts && npm run build-css",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublish": "npm run test:nowatch && npm run lint && npm run build",
    "build-es": "rollup --config rollup.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.4.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/addon-onboarding": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/preset-create-react-app": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@storybook/react-webpack5": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.5.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^16.18.97",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/parser": "^5.0.1",
    "async-validator": "^4.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-plugin-storybook": "^0.8.0",
    "lodash-es": "^4.17.21",
    "node-sass": "^9.0.0",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "prop-types": "15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "rimraf": "^5.0.7",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "storybook": "^8.1.3",
    "typescript": "^4.4.4",
    "web-vitals": "^2.1.4",
    "webpack": "5.91.0"
  }
}
